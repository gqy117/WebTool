@{
    string controller = Request.Get(x => x.RequestContext.RouteData.Values["controller"]).ToStringN();
    string action = Request.Get(x => x.RequestContext.RouteData.Values["action"]).ToStringN();
}
<script type="text/javascript">


    function SendPageSpeedTimings(controller,action) {

        var pageEndTime = new Date().getTime();
        var timeSpent = pageEndTime - pageStartTime;
        ga('send', 'timing', controller, action, timeSpent);
    }

    Add_JS_Content('SendPageSpeedTimings("@controller","@action");');
</script>
